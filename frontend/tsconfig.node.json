{
  "compilerOptions": {
    "composite": true,
    "skipLibCheck": true,
    "module": "ESNext",
    "moduleResolution": "bundler",
    "allowSyntheticDefaultImports": true,
    "types": ["node"],
    "esModuleInterop": true,
    "allowImportingTsExtensions": true,
    "emitDeclarationOnly":true,
    // "noEmit": true,
    "strict": true
  },
  "include": [
    "vite.config.ts",
    "tailwind.config.js",
    "postcss.config.js"
  ]
}